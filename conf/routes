# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                     		walrath.technology.openwalrus.controllers.Application.index
GET     /create                     walrath.technology.openwalrus.controllers.Application.index2
GET		/login/:handle/:password	walrath.technology.openwalrus.controllers.Application.read(handle: String, password:String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
